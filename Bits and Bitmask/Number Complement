Approach 1 : Accepted
class Solution {
    public int findComplement(int num) {
        int temp=num;
        StringBuilder flippedBinaryReverse=new StringBuilder();
        
        while(temp>0){
            if((temp&1)==0){
                flippedBinaryReverse.append("1");
            }else if((temp&1)==1){
                flippedBinaryReverse.append("0");
            }
            
            temp=temp>>1;
        }
        
        String flippedBinary=flippedBinaryReverse.reverse().toString();
        
        int ans=0;
        int place=0;
        for(int i=flippedBinary.length()-1;i>=0;i--){
            int quotient=(int)Math.pow(2,place);
            place++;
            ans+=quotient*(flippedBinary.charAt(i)-'0');
        }
        return ans;
        
    }
}
