class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
        int[][] ans=new int[m][n];
        int length=original.length;
        if(length!=m*n) return new int[][]{};
        

        int currIndex=0;
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                ans[i][j]=original[currIndex];
                currIndex++;
            }
        }
        
        return ans;
    }
} 
