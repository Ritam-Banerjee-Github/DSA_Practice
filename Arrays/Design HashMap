class Pair{
    int key;
    int value;
    
    public Pair(int key, int value){
        this.key=key;
        this.value=value;
    }
}


class MyHashMap {
    List<List<Pair>> list;
    int capacity=100000; //Atmost 10^4 calls will be made in total
    
    public MyHashMap() {
        list=new ArrayList<>();
        for(int i=0;i<capacity;i++){
            list.add(new ArrayList<Pair>());
        }
    }
    
    public void put(int key, int value) {
        int index=key%capacity;
        
        List<Pair> currList=list.get(index);
        for(Pair p:currList){
            if(p.key==key){
                p.value=value;
                return;
            }
            
        }
        
        currList.add(new Pair(key,value));
    }
    
    public int get(int key) {
        int index=key%capacity;
        
        List<Pair> currList=list.get(index);
        for(Pair p:currList){
            if(p.key==key) return p.value;
            
        }
        
        return -1;
    }
    
    public void remove(int key) {
        int index=key%capacity;
        
        List<Pair> currList=list.get(index);
        for(Pair p:currList){
            if(p.key==key) currList.remove(p);
            
            return;
            
        }
    }
}

/**
 * Your MyHashMap object will be instantiated and called as such:
 * MyHashMap obj = new MyHashMap();
 * obj.put(key,value);
 * int param_2 = obj.get(key);
 * obj.remove(key);
 */
