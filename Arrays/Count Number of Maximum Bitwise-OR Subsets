class Solution {
    int reqOr=0;
    
    private int computeSubsetCount(int i,int currOr,int[] nums, int n){
        if(i==n) {
            if(currOr==reqOr) return 1;
            return 0;
        }
        
        int take=computeSubsetCount(i+1,currOr,nums,n);
        int notTake=computeSubsetCount(i+1,currOr|nums[i],nums,n);
        
        return take+notTake;
    }
    public int countMaxOrSubsets(int[] nums) {
        int n=nums.length;
        int maxOr=0;
        for(int i=0;i<n;i++){
            maxOr=maxOr|nums[i];    //For OR - max value means more number of OR with elements. For AND - min  value means more number of AND with elements.
        }
        this.reqOr=maxOr;
        return computeSubsetCount(0,0,nums,n);
      
    }
}
